{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Malaysia 5-Star Restaurants",
  "width": 800,
  "height": 400,
  "projection": {"type": "equalEarth"},
  "data": {
    "url": "https://raw.githubusercontent.com/booi0003/3179/refs/heads/main/homework%20week%209/ne_10m_admin_1_states_provinces.json",
    "format": {"type": "topojson", "feature": "ne_10m_admin_1_states_provinces"}
  },
  "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/booi0003/3179/refs/heads/main/homework%20week%209/Malaysia%20Restaurant%20Review%20Datasets/data_cleaned/five_star_count_by_state.csv",
          "format": {"type": "csv"}
        },
        "key": "Location",
        "fields": ["5star_review_count"]
      }
    }
  ],
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "5star_review_count",
      "type": "quantitative",
      "scale": {
        "type": "threshold",
        "domain": [1000, 10000, 20000],
        "range": ["#d9d9d9", "#f7fcb9", "#fee08b", "#fdae61", "#d73027"]
      }
    },
    "tooltip": [
      {"field": "properties.name", "type": "nominal", "title": "Location"},
      {"field": "5star_review_count", "type": "quantitative"}
    ]
  },
  "config": {}
}